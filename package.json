    "pretest-cover": "npm run create-symlink",
    "test-cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha",
    "posttest-cover": "npm run remove-symlink",
    "jshint": "./node_modules/.bin/jshint lib/*.js test/*.js"
